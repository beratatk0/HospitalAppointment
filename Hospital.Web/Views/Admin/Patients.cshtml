@using Hospital.Core.Models;
@model IEnumerable<PatientIdentity>

<div class="container">
    <table class="table table-stripped">
        <thead>
            <tr>
                <td>Id</td>
                <td>Name</td>
                <td>Username</td>
                <td>City</td>
                <td>Gender</td>
                <td>Age</td>
                <td>Date of Birth</td>
                <td>Email</td>
                <td>Email Confirmation</td>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.Name</td>
                    <td>@item.UserName</td>
                    <td>@item.City</td>
                    <td>@item.Sex</td>
                    <td>@item.Age</td>
                    <td>@item.DateOfBirth</td>
                    <td>@item.Email</td>
                    <td>@item.EmailConfirmed</td>
                </tr>
            }
        </tbody>
    </table>
</div>
